  "Hiragino Sans GB", "Source Han Sans CN", 
  "PingFang SC", Roboto,  "Heiti SC",
   "Microsoft Yahei", sans-serif;
  background:#fff;
}
.finish{
  width: 80%;
  margin:5px auto;
  line-height: 40px;
  font-size: 15px;
  border-radius: 2px;
  color: #ffffff;
  text-align: center;
  background: #c79147;
}
.good_name{
  height: 50px;
}
.good_price{
  margin-top:10px;
  color: red;
  font-size: 25px;
}
.collect_img{
  width: 25px;
  height: 25px;

}
.coltxt{

}
.collect_0{
  background: url(http://bmob-cdn-14091.b0.upaiyun.com/2017/09/14/75e8b3f4405afd9c80c6297a617d4d44.png) no-repeat;
}
.collect_1{
  background: url(http://bmob-cdn-14091.b0.upaiyun.com/2017/09/14/8cffa26240cad54b80c485979a8dcd0f.png) no-repeat;
}

.comment{
    padding: 10rpx 40rpx;
    border-bottom: 3px solid #f5f6f7;
    background: #ffffff;
    margin-top: 10px;
    padding-bottom: 45px;
}
.people{
  flex-direction: row;
  display: flex;
  width: 100%;
  align-items: center;
  margin-top: 10rpx;
}
.comment_num{
  font-size:14px;
}
.comment_img{
  width: 98rpx;
  height: 98rpx;
  border-radius: 50%;
}
.comment_name{
  line-height: 98rpx;
  font-size:15px;
